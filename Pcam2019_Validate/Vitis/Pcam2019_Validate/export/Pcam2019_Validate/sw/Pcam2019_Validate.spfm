<?xml version="1.0" encoding="UTF-8"?>
<sdx:platform sdx:vendor="xilinx.com"
              sdx:library="sdx"
              sdx:name="Pcam2019_Validate"
              sdx:version="1.0"
              sdx:schemaVersion="1.0"
              xmlns:sdx="http://www.xilinx.com/sdx">
    <sdx:description>
Pcam2019_Validate
    </sdx:description>
    <sdx:systemConfigurations sdx:defaultConfiguration="Pcam2019_Validate">
        <sdx:configuration sdx:name="Pcam2019_Validate"
                           sdx:displayName="Pcam2019_Validate"
                           sdx:defaultProcessorGroup="standalone_domain"
                           sdx:runtimes="cpp">
            <sdx:description>Pcam2019_Validate</sdx:description>
            <sdx:bootImages sdx:default="standard">
                <sdx:image sdx:name="standard"
                            sdx:bif="Pcam2019_Validate/boot/Pcam2019_Validate.bif"
                            sdx:readme="Pcam2019_Validate/boot/generic.readme"
                            sdx:qemuArguments="Pcam2019_Validate/qemu/qemu_args.txt"
                            />
                <sdx:image sdx:name="standalone_domain"
                            sdx:bif="Pcam2019_Validate/boot/Pcam2019_Validate.bif"
                            sdx:readme="Pcam2019_Validate/boot/generic.readme"
                            sdx:qemuArguments="Pcam2019_Validate/standalone_domain/qemu/qemu_args.txt"
                            />
            </sdx:bootImages>
            <sdx:processorGroup sdx:name="standalone_domain"
                                sdx:displayName="standalone on ps7_cortexa9_0"
                                sdx:cpuType="cortex-a9"
                                sdx:cpuInstance="ps7_cortexa9_0">
                <sdx:os sdx:name="standalone"
                        sdx:displayName="standalone"
                        sdx:bspLibraryPaths="Pcam2019_Validate/standalone_domain/bsplib/lib"
                        sdx:bspIncludePaths="Pcam2019_Validate/standalone_domain/bspinclude/include"
                        sdx:bspConfig="Pcam2019_Validate/standalone_domain/system.mss"
                />
            </sdx:processorGroup>
        </sdx:configuration>
    </sdx:systemConfigurations>
</sdx:platform>
