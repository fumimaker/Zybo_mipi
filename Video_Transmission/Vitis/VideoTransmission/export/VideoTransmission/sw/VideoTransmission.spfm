<?xml version="1.0" encoding="UTF-8"?>
<sdx:platform sdx:vendor="xilinx.com"
              sdx:library="sdx"
              sdx:name="VideoTransmission"
              sdx:version="1.0"
              sdx:schemaVersion="1.0"
              xmlns:sdx="http://www.xilinx.com/sdx">
    <sdx:description>
VideoTransmission
    </sdx:description>
    <sdx:systemConfigurations sdx:defaultConfiguration="VideoTransmission">
        <sdx:configuration sdx:name="VideoTransmission"
                           sdx:displayName="VideoTransmission"
                           sdx:defaultProcessorGroup="standalone_domain"
                           sdx:runtimes="cpp">
            <sdx:description>VideoTransmission</sdx:description>
            <sdx:bootImages sdx:default="standard">
                <sdx:image sdx:name="standard"
                            sdx:bif="VideoTransmission/boot/VideoTransmission.bif"
                            sdx:readme="VideoTransmission/boot/generic.readme"
                            sdx:qemuArguments="VideoTransmission/qemu/qemu_args.txt"
                            />
                <sdx:image sdx:name="standalone_domain"
                            sdx:bif="VideoTransmission/boot/VideoTransmission.bif"
                            sdx:readme="VideoTransmission/boot/generic.readme"
                            sdx:qemuArguments="VideoTransmission/standalone_domain/qemu/qemu_args.txt"
                            />
            </sdx:bootImages>
            <sdx:processorGroup sdx:name="standalone_domain"
                                sdx:displayName="standalone_domain"
                                sdx:cpuType="cortex-a9"
                                sdx:cpuInstance="ps7_cortexa9_0">
                <sdx:os sdx:name="standalone"
                        sdx:displayName="standalone"
                        sdx:bspLibraryPaths="VideoTransmission/standalone_domain/bsplib/lib"
                        sdx:bspIncludePaths="VideoTransmission/standalone_domain/bspinclude/include"
                        sdx:bspConfig="VideoTransmission/standalone_domain/system.mss"
                />
            </sdx:processorGroup>
        </sdx:configuration>
    </sdx:systemConfigurations>
</sdx:platform>
